{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.;Initial Catalog=BlogDataBase;Integrated Security=True;trusted_connection=true;"
  },
  "IdentityConfiguration": {
    "PasswordRequireDigit": false,
    "PasswordRequireLowercase": false,
    "PasswordRequireNonAlphanumeric": false,
    "PasswordRequireUppercase": false,
    "LockoutAllowedForNewUsers": false,
    "UserRequireUniqueEmail": false,
    "PasswordRequiredUniqueChars": 1,
    "PasswordMinLength": 8,
    "LockoutMaxFailedAccessAttempts": 3,
    "UserAllowedUserNameCharacters": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789",
    "LockoutDefaultLockoutTimeSpan": 20,
    "LockoutType": "Minute"
  },
  "CorsConfiguration": {
    "AllowAllOrigins": true,
    "AllowAllHeaders": true,
    "AllowAllMethods": true,
    "Origins": "",
    "Headers": "",
    "Methods": ""
  }
}
